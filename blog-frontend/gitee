cd D:\blog-wintertide
# 重新初始化本地仓库
rm -rf .git
git init
git branch
git checkout main

# 添加所有文件
git add .
git push -f origin main:bagin
# 提交更改（使用正确的分支名称）
git checkout -b master
git commit -m "前后端项目提交，布置上云"

# 设置远程仓库（使用图片中显示的 blog-wintertide）
git remote add origin https://gitee.com/lwinterl/blog-wintertide.git

# 强制推送
git push -f origin main
git push -f origin master






git remote add github https://github.com/IIwinterII/blog-wintertide.git

# 推送本地 main 分支到 GitHub 远程的 main 分支
git push github main







# 删除主项目的 .git 文件夹（强制删除）
Remove-Item -Recurse -Force .git

# 删除前端项目中的 .git 文件夹（如果存在）
Remove-Item -Recurse -Force blog-frontend/.git -ErrorAction SilentlyContinue

# 删除后端项目中的 .git 文件夹（如果存在）
Remove-Item -Recurse -Force blog-backend/.git -ErrorAction SilentlyContinue

# 删除 .gitmodules 文件（如果存在）
Remove-Item .gitmodules -ErrorAction SilentlyContinue